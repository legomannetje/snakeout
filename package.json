{
  "name": "spyout",
  "version": "1.0.0",
  "main": "src/server/app.js",
  "scripts": {
    "start": "set NODE_ENV=production&& npm run build && npm run server",
    "build": "webpack --mode production",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "node src/server/bin/www",
    "dev": "set NODE_ENV=development&& concurrently \"nodemon --exec npm run server\" \"npm run client\"",
    "test": "tape src/server/test/*.js",
    "precommit": "pretty-quick --staged"
  },
  "author": "Tanner Krewson",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^4.1.1",
    "debug": "~2.2.0",
    "express": "^4.16.3",
    "knuth-shuffle": "^1.0.1",
    "morgan": "~1.7.0",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-router-dom": "^4.2.2",
    "socket.io": "^1.4.8",
    "socket.io-client": "^1.4.8"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.11",
    "exports-loader": "^0.7.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "nodemon": "^1.17.3",
    "prettier": "1.13.4",
    "pretty-quick": "^1.6.0",
    "style-loader": "^0.20.3",
    "tape": "^4.9.0",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.3"
  },
  "description": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tannerkrewson/spyout.git"
  },
  "bugs": {
