{
	"name": "snakeout",
	"version": "1.0.0",
	"main": "src/server/app.js",
	"scripts": {
		"start": "run-s build:prod start:prod",
		"dev": "run-p start:dev build:dev",
		"start:dev": "nodemon --exec npm run start:prod",
		"build:dev": "webpack --mode development --watch",
		"start:prod": "node src/server/bin/www",
		"build:prod": "webpack --mode production",
		"test": "tape src/server/test/*.js"
	},
	"author": "Tanner Krewson",
	"license": "MIT",
	"dependencies": {
		"@babel/core": "^7.0.0",
		"@babel/preset-env": "^7.0.0",
		"@babel/preset-react": "^7.0.0",
		"babel-loader": "^8.0.6",
		"bootstrap": "^4.4.1",
		"clean-webpack-plugin": "^3.0.0",
		"css-loader": "^3.4.0",
		"debug": "~4.1.1",
		"express": "^4.16.3",
		"html-webpack-plugin": "^3.2.0",
		"knuth-shuffle": "^1.0.1",
		"morgan": "~1.9.1",
		"npm-run-all": "^4.1.5",
		"react": "^16.3.1",
		"react-dom": "^16.12.0",
		"react-icons": "^3.8.0",
		"react-router-dom": "^5.1.2",
		"socket.io": "^2.3.0",
		"socket.io-client": "^2.3.0",
		"style-loader": "^1.1.2",
		"webpack": "^4.41.5",
		"webpack-cli": "^3.3.10"
	},
	"devDependencies": {
		"exports-loader": "^0.7.0",
		"husky": "^3.1.0",
		"nodemon": "^2.0.2",
		"prettier": "1.19.1",
		"pretty-quick": "^2.0.1",
		"tape": "^4.9.0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/tannerkrewson/snakeout.git"
	},
	"bugs": {
		"url": "https://github.com/tannerkrewson/snakeout/issues"
	},
	"homepage": "https://snakeout.tannerkrewson.com/",
	"husky": {
		"hooks": {
			"pre-commit": "pretty-quick --staged"
		}
	}
}
