{
	"name": "snakeout",
	"version": "1.0.0",
	"main": "src/server/app.js",
	"scripts": {
		"start": "npm run build && npm run server",
		"build": "webpack --mode production",
		"client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
		"server": "node src/server/bin/www",
		"dev": "concurrently \"nodemon --exec npm run server\" \"npm run client\"",
		"dev2": "webpack --mode development && node --inspect src/server/bin/www",
		"dev2mon": "nodemon --watch src --exec npm run dev2",
		"test": "tape src/server/test/*.js",
		"precommit": "pretty-quick --staged"
	},
	"author": "Tanner Krewson",
	"license": "MIT",
	"dependencies": {
		"@babel/core": "^7.0.0",
		"@babel/preset-env": "^7.0.0",
		"@babel/preset-react": "^7.0.0",
		"babel-loader": "^8.0.6",
		"bootstrap": "^4.4.1",
		"clean-webpack-plugin": "^3.0.0",
		"css-loader": "^3.4.0",
		"debug": "~4.1.1",
		"express": "^4.16.3",
		"html-webpack-plugin": "^3.2.0",
		"knuth-shuffle": "^1.0.1",
		"morgan": "~1.9.1",
		"react": "^16.3.1",
		"react-dom": "^16.12.0",
		"react-icons": "^3.8.0",
		"react-router-dom": "^5.1.2",
		"socket.io": "^2.3.0",
		"socket.io-client": "^2.3.0",
		"style-loader": "^1.1.2",
		"webpack": "^4.41.5",
		"webpack-cli": "^3.3.10"
	},
	"devDependencies": {
		"concurrently": "^5.0.2",
		"exports-loader": "^0.7.0",
		"husky": "^3.1.0",
		"nodemon": "^2.0.2",
		"prettier": "1.19.1",
		"pretty-quick": "^2.0.1",
		"tape": "^4.9.0",
		"webpack-dev-server": "^3.10.1"
	},
	"description": "",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/tannerkrewson/snakeout.git"
	},
	"bugs": {
		"url": "https://github.com/tannerkrewson/snakeout/issues"
	},
	"homepage": "https://github.com/tannerkrewson/snakeout#readme"
}
